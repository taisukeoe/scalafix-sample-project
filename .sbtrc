alias execScalafix=;refactor/scalafix;refactor/Test/scalafix
alias checkScalafix=;refactor/scalafix --check;refactor/Test/scalafix --check
alias strictCompile=;set Compile/compile/scalacOptions += "-Xfatal-warnings";compile
alias strictRecompile=;set Compile/compile/scalacOptions += "-Xfatal-warnings";~compile
alias checkAll=;strictCompile;scalafmtCheckAll;scalafmtSbtCheck;checkScalafix;test
